{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "max-classes-per-file":0,
    "comma-dangle": 0
  },
  "globals": {
    "window": true,
    "document": true,
    "browser": false
  }
}
